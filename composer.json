{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anlutro/l4-settings": "*",
        "ashallendesign/short-url": "^7.6",
        "barryvdh/laravel-dompdf": "^2.0",
        "biscolab/laravel-recaptcha": "*",
        "giauphan/laravel-qr-code": "^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "intervention/image": "^2.5",
        "jackiedo/dotenv-editor": "^2.0",
        "jenssegers/agent": "^2.6",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.9",
        "laravel/sanctum": "^3.3",
        "laravel/slack-notification-channel": "^3.0",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "pragmarx/countries": "^0.8.2",
        "pragmarx/countries-laravel": "^0.7.0",
        "predis/predis": "^2.0",
        "propaganistas/laravel-phone": "^5.0",
        "pusher/pusher-php-server": "^7.0",
        "rap2hpoutre/fast-excel": "^5.2",
        "rap2hpoutre/laravel-log-viewer": "^2.2",
        "riverskies/laravel-mobile-detect": "^1.3",
        "sentry/sentry-laravel": "^4.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.4",
        "spatie/laravel-cookie-consent": "^3.2",
        "spatie/laravel-html": "^3.2",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.9",
        "spatie/laravel-short-schedule": "^1.3",
        "spatie/period": "^2.4",
        "tightenco/ziggy": "^1.1",
        "webpatser/laravel-uuid": "^4.0",
        "yajra/laravel-datatables-oracle": "^10.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
